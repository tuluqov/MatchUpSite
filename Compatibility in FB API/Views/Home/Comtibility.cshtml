@using System.Globalization
@model MatchUp.Models.CompatibilityViewModel

@{
    ViewBag.Title = "Comtibility";
    Layout = "../Shared/_Layout.cshtml";
}
<div class="container">
    <div class="form-group border-bottom pt-2">
        <h2>Comtibility</h2>
    </div>
    @if (Model != null)
    {
        <div class="row">
            <div class="col text-center">
                <a href="~/home/userpage/@Model.User1.Id">
                    <img src="@Model.User1.PhotoUrl" class="round mb-2 mt-2">
                    <h6>@Model.User1.Name</h6>
                    <h6>@Model.User1.Birthday.ToString("MMMM d, yyyy", CultureInfo.GetCultureInfoByIetfLanguageTag("en-US"))</h6>
                </a>
            </div>
            <div class="col text-center">
                @if (Request.QueryString["isFamouse"] == "True")
                {
                    <a href="~/home/userpage/@Model.User2.Id?isFamouse=true">
                        <img src="@Model.User2.PhotoUrl" class="round mb-2 mt-2">
                        <h6>@Model.User2.Name</h6>
                        <h6>@Model.User2.Birthday.ToString("MMMM d, yyyy", CultureInfo.GetCultureInfoByIetfLanguageTag("en-US"))</h6>
                    </a>
                }
                else
                {
                    <a href="~/home/userpage/@Model.User2.Id">
                        <img src="~/Content/img/ava.png" class="round mb-2 mt-2">
                        <h6>@Model.User2.Name</h6>
                        <h6>@Model.User2.Birthday.ToString("MMMM d, yyyy", CultureInfo.GetCultureInfoByIetfLanguageTag("en-US"))</h6>
                    </a>
                }

            </div>
        </div>
        <div class="card mt-2">
            @if (!(Math.Abs(Model.User1.PythagorianMatrix.CharacterWill.Count(x => x == '1') - Model.User2.PythagorianMatrix.CharacterWill.Count(x => x == '1')) > 2 &&
                Math.Abs(Model.User1.PythagorianMatrix.CharacterWill.Count(x => x == '1') - Model.User2.PythagorianMatrix.CharacterWill.Count(x => x == '1')) < 5))
            {
                <div class="card-header">
                    Compatible by character: (@Model.User1.PythagorianMatrix.CharacterWill:@Model.User2.PythagorianMatrix.CharacterWill)
                    <img src="data:image/png;base64,">
                </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас слабая совместимость по характеру, если часто возникают ссоры, стоит задуматься.</p>
                    </blockquote>
                </div>
            }
            else
            {
                <div class="card-header">
                    Compatible by character: (@Model.User1.PythagorianMatrix.CharacterWill:@Model.User2.PythagorianMatrix.CharacterWill)
                    <img src="data:image/png;base64,">

                </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас отличная совместимость по характеру, по типу характера, это близкий вам человек.</p>
                    </blockquote>
                </div>
            }
        </div>

        <div class="card mt-2">
            @if (!(Math.Abs(Model.User1.SecondaryAbilities.Temperament - Model.User2.SecondaryAbilities.Temperament) < 2))
            {
                <div class="card-header">
                    Temperament compatibility: (@Model.User1.SecondaryAbilities.Temperament : @Model.User2.SecondaryAbilities.Temperament)
                    <img src="data:image/png;base64,">
                </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас слабая совместимость по темпераменту.</p>
                    </blockquote>
                </div>
            }
            else
            {
                <div class="card-header">
                    Temperament compatibility: (@Model.User1.SecondaryAbilities.Temperament : @Model.User2.SecondaryAbilities.Temperament)
                    <img src="data:image/png;base64,">
                </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас отличная совместимость по темпераменту, по темпераменту, это близкий вам человек.</p>
                    </blockquote>
                </div>
            }

        </div>

        <div class="card mt-2">
            @if (!(Math.Abs(Model.User1.SecondaryAbilities.DomesticBliss - Model.User2.SecondaryAbilities.DomesticBliss) < 2))
            {
                <div class="card-header">
                    Family compatibility: (@Model.User1.SecondaryAbilities.DomesticBliss : @Model.User2.SecondaryAbilities.DomesticBliss)
                    <img src="data:image/png;base64,">
                </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас слабая совместимость по семейности, немного разное отношение к семейным ценностям.</p>
                    </blockquote>
                </div>
            }
            else
            {
                <div class="card-header">
                    Family compatibility: (@Model.User1.SecondaryAbilities.Tenacity : @Model.User2.SecondaryAbilities.Tenacity)
                    <img src="data:image/png;base64,">

                </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас отличная совместимость семейных ценностей, похоже ваши семейные ценности близки.</p>
                    </blockquote>
                </div>
            }

        </div>
    }
</div>
