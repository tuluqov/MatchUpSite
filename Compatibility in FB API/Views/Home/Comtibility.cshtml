@model MatchUp.Models.CompatibilityViewModel

@{
    ViewBag.Title = "Comtibility";
    Layout = "../Shared/_Layout.cshtml";
}
<div class="container">
    <h2>Совместимость</h2>
    <div class="form-group border-top pt-2">
        @using (Html.BeginForm("Comtibility", "Home", FormMethod.Post))
        {
            <div class="form-group mt-2">
                <input type="text" required name="Name" id="Name" placeholder="Введите имя" class="form-control" />
            </div>
            <div class="row">
                <h5 class="col-4 m-2 align-middle">Введите дату рождения:</h5>

                <select class="form-control m-2 col-2" name="day">
                    @for (int i = 1; i <= 31; i++)
                    {
                        <option>@i</option>
                    }
                </select>
                <select class="form-control m-2 col-2" name="month">
                    @for (int i = 1; i <= 12; i++)
                    {
                        <option>@i</option>
                    }
                </select>
                <select class="form-control m-2 col-2" name="year">
                    @for (int i = 1900; i <= 2018; i++)
                    {
                        <option>@i</option>
                    }
                </select>
                <button type="submit" class="btn btn-primary m-2 col">Сравнить</button>
            </div>
        }
    </div>
    @if (Model != null)
    {
        <div class="card mt-2">
            @if (Math.Abs(Model.User1.Matrix[1].Count(x => x != '-') - Model.User2.Matrix[1].Count(x => x != '-')) < 2)
            {
                <div class="card-header">
                    Совместимость по характеру: (@Model.User1.Matrix[1]:@Model.User2.Matrix[1])
                    <img src="data:image/png;base64,">
                </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас слабая совместимость по характеру, если часто возникают ссоры, стоит задуматься.</p>
                    </blockquote>
                </div>
            }
            else
            {
                <div class="card-header">
                    Совместимость по характеру: (@Model.User1.Matrix[1]:@Model.User2.Matrix[1])
                    <img src="data:image/png;base64,">

                </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас отличная совместимость по характеру, по типу характера, это близкий вам человек.</p>
                    </blockquote>
                </div>
            }
        </div>

        <div class="card mt-2">
            @if (Math.Abs(Model.User1.SecondaryAbilities.Temperament - Model.User2.SecondaryAbilities.Temperament) > 2)
            {
            <div class="card-header">
                Совместимость по темпераменту: (@Model.User1.SecondaryAbilities.Temperament : @Model.User2.SecondaryAbilities.Temperament)
                <img src="data:image/png;base64,">
            </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас слабая совместимость по темпераменту.</p>
                    </blockquote>
                </div>
            }
            else
            {
                <div class="card-header">
                    Совместимость по темпераменту: (@Model.User1.SecondaryAbilities.Temperament : @Model.User2.SecondaryAbilities.Temperament)
                    <img src="data:image/png;base64,">
                </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас отличная совместимость по темпераменту, по темпераменту, это близкий вам человек.</p>
                    </blockquote>
                </div>
            }

        </div>
        
        <div class="card mt-2">
            @if (Math.Abs(Model.User1.SecondaryAbilities.Tenacity - Model.User2.SecondaryAbilities.Tenacity) > 2)
            {
                <div class="card-header">
                    Совместимость по семейности: (@Model.User1.SecondaryAbilities.Tenacity : @Model.User2.SecondaryAbilities.Tenacity)
                    <img src="data:image/png;base64,">
                </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас слабая совместимость по семейности, немного разное отношение к семейным ценностям.</p>
                    </blockquote>
                </div>
            }
            else
            {
                <div class="card-header">
                    Совместимость по семейности: (@Model.User1.SecondaryAbilities.Tenacity : @Model.User2.SecondaryAbilities.Tenacity)
                    <img src="data:image/png;base64,">

                </div>
                <div class="card-body">
                    <blockquote class="blockquote mb-0">
                        <p>У вас отличная совместимость семейных ценностей, похоже ваши семейные ценности близки.</p>
                    </blockquote>
                </div>
            }

        </div>
    }
</div>
